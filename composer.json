{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-mongodb": "*",
        "api-platform/doctrine-odm": "^4.1.20",
        "api-platform/doctrine-orm": "^4.1.20",
        "api-platform/symfony": "^4.1.20",
        "doctrine/dbal": "^4.3.2",
        "doctrine/doctrine-bundle": "^2.15.1",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/mongodb-odm": ">=2.11.2",
        "doctrine/mongodb-odm-bundle": ">=5.3.1",
        "doctrine/orm": "^3.5.2",
        "dragonmantank/cron-expression": "^3.4",
        "league/flysystem-bundle": "^3.5",
        "league/flysystem-memory": "^3.29",
        "nelmio/cors-bundle": "^2.5",
        "nesbot/carbon": "^3.10.2",
        "phpdocumentor/reflection-docblock": "^5.6.2",
        "phpstan/phpdoc-parser": "^2.2",
        "symfony/asset": "7.2.*",
        "symfony/asset-mapper": "7.2.*",
        "symfony/console": "7.2.*",
        "symfony/doctrine-messenger": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/expression-language": "7.2.*",
        "symfony/flex": "^2.8.1",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-client": "7.2.*",
        "symfony/intl": "7.2.*",
        "symfony/mailer": "7.2.*",
        "symfony/mime": "7.2.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "7.2.*",
        "symfony/process": "7.2.*",
        "symfony/property-access": "7.2.*",
        "symfony/property-info": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/scheduler": "7.2.*",
        "symfony/security-bundle": "7.2.*",
        "symfony/serializer": "7.2.*",
        "symfony/stimulus-bundle": "^2.29.1",
        "symfony/string": "7.2.*",
        "symfony/translation": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/ux-turbo": "^2.29.1",
        "symfony/validator": "7.2.*",
        "symfony/web-link": "7.2.*",
        "symfony/workflow": "7.2.*",
        "symfony/yaml": "7.2.*",
        "twig/extra-bundle": "^2.12|^3.21",
        "twig/twig": "^2.12|^3.21.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.85.1",
        "phpunit/phpunit": "^12.3.1",
        "symfony/browser-kit": "7.2.*",
        "symfony/css-selector": "7.2.*",
        "symfony/debug-bundle": "7.2.*",
        "symfony/maker-bundle": "^1.64.0",
        "symfony/phpunit-bridge": "^7.3.1",
        "symfony/stopwatch": "7.2.*",
        "symfony/web-profiler-bundle": "7.2.*",
        "vimeo/psalm": "^6.13.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "tests/Fixture/Datasets/CVE/Schema/affected.php",
            "tests/Fixture/Datasets/CVE/Schema/affected_routine.php",
            "tests/Fixture/Datasets/CVE/Schema/affected_version.php",
            "tests/Fixture/Datasets/CVE/Schema/affected_version_change.php",
            "tests/Fixture/Datasets/CVE/Schema/cna.php",
            "tests/Fixture/Datasets/CVE/Schema/cpe_applicability.php",
            "tests/Fixture/Datasets/CVE/Schema/cpe_match.php",
            "tests/Fixture/Datasets/CVE/Schema/cpe_node.php",
            "tests/Fixture/Datasets/CVE/Schema/credit.php",
            "tests/Fixture/Datasets/CVE/Schema/cvss_2_0.php",
            "tests/Fixture/Datasets/CVE/Schema/cvss_3_0.php",
            "tests/Fixture/Datasets/CVE/Schema/cvss_3_1.php",
            "tests/Fixture/Datasets/CVE/Schema/cvss_4_0.php",
            "tests/Fixture/Datasets/CVE/Schema/description.php",
            "tests/Fixture/Datasets/CVE/Schema/description_media.php",
            "tests/Fixture/Datasets/CVE/Schema/impact.php",
            "tests/Fixture/Datasets/CVE/Schema/metric.php",
            "tests/Fixture/Datasets/CVE/Schema/metric_scenario.php",
            "tests/Fixture/Datasets/CVE/Schema/other_metric.php",
            "tests/Fixture/Datasets/CVE/Schema/problem_description.php",
            "tests/Fixture/Datasets/CVE/Schema/problem_type.php",
            "tests/Fixture/Datasets/CVE/Schema/provider_metadata.php",
            "tests/Fixture/Datasets/CVE/Schema/record.php",
            "tests/Fixture/Datasets/CVE/Schema/record_containers.php",
            "tests/Fixture/Datasets/CVE/Schema/record_metadata.php",
            "tests/Fixture/Datasets/CVE/Schema/reference.php",
            "tests/Fixture/Datasets/CVE/Schema/taxonomy_mapping.php",
            "tests/Fixture/Datasets/CVE/Schema/taxonomy_relation.php",
            "tests/Fixture/Datasets/CVE/Schema/timeline.php"
        ]
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "tests:run" : "@php bin/phpunit",
        "tests:coverage": "@php bin/phpunit --coverage-text",
        "tests:unit:run": "@php bin/phpunit --testsuite unit",
        "tests:e2e:run": "@php bin/phpunit --testsuite e2e",
        "code:style:analyze": "@php vendor/bin/php-cs-fixer --config=.php-cs-fixer.php check src tests",
        "code:style:fix": "@php vendor/bin/php-cs-fixer --config=.php-cs-fixer.php fix src tests",
        "code:analyze": "@php vendor/bin/psalm",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "7.2.*"
        }
    }
}
